.container{
margin:0px;
padding:0px;
width:100%;
}
/* ----------------------- First Div ----------------------- */
#first {background-image:linear-gradient(rgba(0,0,0,0.1),rgba(0,0,0,0.5)),url("../Images/3d.png");height: 565px;width: 100%;text-transform: uppercase;}
#first p {color: white;font-size: 80px;text-align: center;padding-top: 110px;font-weight: 600}
#first span {color: white;font-size: 40px;padding-left: 665px;font-weight: 600}
#things{font-size: 60px;color: #25688e;text-align: center;margin: 50px;text-transform: capitalize;}
/* ----------------------- Second Div ----------------------- */
#second h1 {font-size: 50px;color: #25688e;text-align: center;margin: 50px ;text-transform: capitalize;margin-bottom:0px;} 
#map {padding: 30px 0px 50px 100px ;display: inline-block;}
#cont {display: inline-block;}
#cont img {width: 25px;height: 25px;padding-left: 50px; padding-right: 15px}
#cont span { display: inline-block;}

/* ----------------------- Leave Question  -----------------------*/

#leave p {color: #25688e; font-size: 30px;padding: 20px 0px 50px 0px;font-weight: 600}

#leave form input {border: 1px solid #ccc;padding: 25px 30px 25px 30px;width:20%; height: 30px;margin: 0px 20px 0px 0px;font-size: 20px;border-radius: 5px}
textarea {border: 1px solid #ccc;border-radius: 5px ;margin-bottom: 20px}
#send {background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));
    background-color: #25688e; color: white; font-size: 20px; width: 200px;height: 50px;border-radius: 40px;margin-bottom: 80px;border:none;}
	
/* ----------------------- Footer  -----------------------*/
#end {background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));background-color:#74b2d4 ;height: 95px; }
footer #end img {display: inline-block;padding: 10px 0px 0px 30px }



/* ----------------------- media tag -----------------------*/
@media screen and (max-width:1365px){
    
    #last { width: 100%}
    .links{width: 100%}
    .links ul li a { padding: 5px 10px; }
/* ----------------------- First div -----------------------*/
#first { width: 100%}
 
    #second  { width: 100% } 
#map {padding: 30px 0px 50px 10% ;display: inline-block;width: 50%}
#cont {display: inline-block;width: 50%}
#cont img {height: 10%;padding-left: 20%; padding-right: 15px;width: 10%}

        /* ----------------------- Leave Question  -----------------------*/
#leave{margin:15px 0px 0px 10% }
#leave p {font-size: 30px;}
#leave form input {width: 70%;}
     
}
#end{text-align:center;}

@media screen and (max-width:700px){

    #last { width: 100%}
    .links{width: 100%}
    .links ul li{width: 100%;margin: 10px;}
    .links ul li a { padding: 5px 10px; }
    /*#end {width: 100%;padding-left: 0%;font-size: 12px }*/
    
}



.footer {
background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));
background-color: #25688e;
height: auto;
padding-bottom: 30px;
width:100%;
border-bottom: 1px solid #CCCCCC;
border-top: 1px solid #DDDDDD;
}


.footer h3 {
border-bottom: 1px solid #BAC1C8;
    text-align: left;
color: #2874af;
font-size: 18px;
font-weight: 600;
line-height: 27px;
padding: 40px 0 10px;
text-transform: uppercase;
}
.footer ul {
font-size: 13px;
list-style-type: none;
margin-left: 0;
padding-left: 0;
margin-top: 15px;
color: #7F8C8D;
    text-align: left;
}
.footer ul li a {
padding: 0 0 5px 0;
display: block;
}
.footer a {
color: #78828D
}


.social li {
background: none repeat scroll 0 0 #B5B5B5;
border: 2px solid #B5B5B5;
border-radius: 50%;
float: left;
height: 36px;
line-height: 36px;
margin: 0 8px 0 0;
padding: 0;
text-align: center;
width: 36px;
transition: all 0.5s ease 0s;
}
.social li:hover {
transform: scale(1.15) rotate(360deg);
}
.social li a {
color: #EDEFF1;
}
.social li:hover {
border: 2px solid white;
}

.social #face:hover{
background-color:blue;
}

.social #twitter:hover{
background-color: #00CED1;
}

.social #google:hover{
background-color: #FF4500;
}

.social #youtube:hover{
background-color: #FF0000;
}

.social #pinterest:hover{
background-color: #800000;
}


.social li a i {
font-size: 16px;
margin: 0 0 0 5px;
color: #EDEFF1;
}
.footer-bottom {
background: grey;
color: white;
border-top: 1px solid #DDDDDD;
padding-top: 10px;
padding-bottom: 10px;
color: white;

}
.pull-left {
font-size:15px;
margin-right:0%;
}

.pull-right ul{
margin-left:70%;
}
.pull-right ul i{
padding-right:10px;
}

