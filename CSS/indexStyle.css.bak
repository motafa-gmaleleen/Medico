.footer {
	    background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));
background-color: #25688e;
height: auto;
padding-bottom: 30px;
width:100%;
border-bottom: 1px solid #CCCCCC;
border-top: 1px solid #DDDDDD;
}


.footer h3 {
border-bottom: 1px solid #BAC1C8;
color: #2874af;
font-size: 18px;
font-weight: 600;
line-height: 27px;
padding: 40px 0 10px;
text-transform: uppercase;
}
.footer ul {
font-size: 13px;
list-style-type: none;
margin-left: 0;
padding-left: 0;
margin-top: 15px;
color: #7F8C8D;
}
.footer ul li a {
padding: 0 0 5px 0;
display: block;
}
.footer a {
color: #78828D
}


.social li {
background: none repeat scroll 0 0 #B5B5B5;
border: 2px solid #B5B5B5;
border-radius: 50%;
float: left;
height: 36px;
line-height: 36px;
margin: 0 8px 0 0;
padding: 0;
text-align: center;
width: 36px;
transition: all 0.5s ease 0s;
}
.social li:hover {
transform: scale(1.15) rotate(360deg);
}
.social li a {
color: #EDEFF1;
}
.social li:hover {
border: 2px solid white;
}

.social #face:hover{
background-color:blue;
}

.social #twitter:hover{
background-color: #00CED1;
}

.social #google:hover{
background-color: #FF4500;
}

.social #youtube:hover{
background-color: #FF0000;
}

.social #pinterest:hover{
background-color: #800000;
}


.social li a i {
font-size: 16px;
margin: 0 0 0 5px;
color: #EDEFF1;
}
.footer-bottom {
background: grey;
color: white;
border-top: 1px solid #DDDDDD;
padding-top: 10px;
padding-bottom: 10px;
color: white;

}
.pull-left {
font-size:15px;
margin-right:0%;
margin-left: 5%;
}

.pull-right ul{
margin-left:70%;
}
.pull-right ul i{
padding-right:10px;
}