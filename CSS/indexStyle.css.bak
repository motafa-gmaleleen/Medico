.first{background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));background-color:#25688e;width: 100%;height: 100%;  }
.first p span{font-size: 45px;color: white;text-transform:uppercase;display: block;padding-top: 7%;font-weight: 600;line-height: 100px;}
.first p{font-size: 16px;color: white;text-transform:capitalize;line-height: 90px;font-family:"Roboto Slabl" ;text-align: center;padding: 0 10% 0 10%}
.first button a { text-decoration: none;background-color: white;color:#25688e;font-size: 80%; border-radius: 50px;padding-left: 20px}
.first button{border-radius: 50px;background-color: white;padding: 2%;margin-right:45%; margin-bottom:5%;margin-left: 44%;}


.second{background-color: white;height:100%;}
.second p {text-align: center; font-size: 45px;padding-top: 4%;text-transform:uppercase;font-weight: 600;color:#25688e;} 
#fimg {background-image:url("../Images/doctors1.png");height: 300px;width: 350px; display: inline-block; margin-left:5%; margin-right:5%;border-radius: 45%;margin-bottom:2%;} 
#simg {background-image:url("../Images/QandA.png");height: auto;width: auto; display: inline-block;border-radius: 45%;margin-bottom:2%;}
#timg {background-image:url("../Images/child1.png");height: auto;width: auto; display: inline-block;margin-left:5%;border-radius: 45%;margin-bottom:2%;}
#fourimg {background-image:url("../Images/medic.png");height: 300px;display: inline-block; width: 350px;margin-left:8%;margin-top:2%; margin-right:5%; border-radius: 45%;margin-bottom:2%;} 
#fiveimg {background-image:url("../Images/story.png");height: auto;width: auto;margin-left:5%; margin-right:5%;margin-top:2%; display: inline-block;border-radius: 45%;margin-bottom:2%;}
#siximg  {background-image:url("../Images/offer.png");height: auto;width: auto;margin-left:5%; margin-right:5%;margin-top:2%; display: inline-block;border-radius: 45%;margin-bottom:2%;}

.row #t1 { background-color:#f4f4f4 ;width: 350px;margin-left: 5%;line-height: 30px;text-align: center;display: inline-block;}
.row #t1 p{font-size: 20px;margin: 0px; font-weight: 200;}
.row #t1 p span { font-size: 25px;text-align: center ;margin: 0;display:block; padding-top:10%; padding-bottom:10%; font-weight: 600;}
.row #t1 button a { text-decoration: none;background-color: white;color:#25688e;font-size: 60%; border-radius: 50px;padding: 10px}
.row #t1 button{border-radius: 50px;background-color: white;padding: 5%;margin: 5%}

#last {background-image: linear-gradient(rgba(0,0,0,.5),rgba(0,0,0,0.5)),url("../Images/map.png");opacity: .8; ;margin-top: 7%;height: 100%}
#last #lmid1 {text-align:center;}
#last #lmid1 p {color:white ; font-size: 300%;line-height: 85px;text-align: center;font-family: "Roboto"; margin:0;}
#last #lmid2 { height: 100%;margin-right:30%;margin-left: 30% }
