/* ----------------------- First div -----------------------*/
#first { background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5)),url("../Images/log1.png");width: 100%;height: 650px;}

#box { background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));width: 40%;height: 75%;margin-left:32%;border-radius: 10px ;top: 100px;position: relative;}
#box p{ color: white;font-size: 40px;text-align: center;font-weight: 500;padding-top: 40px;padding-bottom: 30px;}
#box form label { color: white; font-size: 24px ;margin-left: 35px;display: block}
#box form  input { width: 88%;height: 25px;margin: 5px 0px 30px 35px;background: transparent;border: none;border-bottom: 1px solid #fff;outline: none;font-size: 16px;color:#e0e0e0}
#box a {text-decoration: none;font-size: 16px;color: #fff ;margin-left: 35px;text-align:right;}
#box a:hover{background: #39dc79}
#bu { border: none;outline: none;height: 40px; width:50%;margin: 35px 0px 30px 25%;background: #1c8adb;color: #fff;font-size: 18px;display: block;border-radius: 20px}
#bu :hover { cursor: pointer;background: #39dc79;color: #000}


/* ----------------------- media tag -----------------------*/
@media screen and (max-width:1365px){
    
    #last { width: 100%}
    .links{width: 100%}
    .links ul li a { padding: 5px 10px; }

/* ----------------------- First div -----------------------*/
#first {width: 100%}
#first img {margin-left: 40%;width: 10%;height: 10%;}
#box { width: 70%;height: 70%;margin-left: 10% ;}
#box p{ font-size: 30px;}
#box form label { font-size: 20px ;margin-left: 10%}
#box form  input { width: 70%;height: 10%;margin: 5px 0px 30px 10%;}
#box a {font-size: 14px;margin-left: 10%}
#bu { border: none;outline: none;height: 10%; width: 70%;margin: 5px 0px 30px 10%;font-size: 18px;}

    
       #end {width: 100%;padding-left: 40% }
}

@media screen and (max-width:700px){

    #last { width: 100%}
    .links{width: 100%}
    .links ul li{width: 100%;margin: 10px;}
    .links ul li a { padding: 5px 10px; }
    #end {width: 100%;padding-left: 0%;font-size: 12px }
    
}




.footer {
background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));
background-color: #25688e;
height: auto;
padding-bottom: 30px;
width:100%;
border-bottom: 1px solid #CCCCCC;
border-top: 1px solid #DDDDDD;
}


.footer h3 {
border-bottom: 1px solid #BAC1C8;
color: #2874af;
font-size: 18px;
font-weight: 600;
line-height: 27px;
padding: 40px 0 10px;
text-transform: uppercase;
}
.footer ul {
font-size: 13px;
list-style-type: none;
margin-left: 0;
padding-left: 0;
margin-top: 15px;
color: #7F8C8D;
}
.footer ul li a {
padding: 0 0 5px 0;
display: block;
}
.footer a {
color: #78828D
}


.social li {
background: none repeat scroll 0 0 #B5B5B5;
border: 2px solid #B5B5B5;
border-radius: 50%;
float: left;
height: 36px;
line-height: 36px;
margin: 0 8px 0 0;
padding: 0;
text-align: center;
width: 36px;
transition: all 0.5s ease 0s;
}
.social li:hover {
transform: scale(1.15) rotate(360deg);
}
.social li a {
color: #EDEFF1;
}
.social li:hover {
border: 2px solid white;
}

.social #face:hover{
background-color:blue;
}

.social #twitter:hover{
background-color: #00CED1;
}

.social #google:hover{
background-color: #FF4500;
}

.social #youtube:hover{
background-color: #FF0000;
}

.social #pinterest:hover{
background-color: #800000;
}


.social li a i {
font-size: 16px;
margin: 0 0 0 5px;
color: #EDEFF1;
}
.footer-bottom {
background: grey;
color: white;
border-top: 1px solid #DDDDDD;
padding-top: 10px;
padding-bottom: 10px;
color: white;

}
.pull-left {
font-size:15px;
margin-right:0%;
margin-left: 5%;
}

.pull-right ul{
margin-left:70%;
}
.pull-right ul i{
padding-right:10px;
}


.col-xs{
	margin-left:6%;
}