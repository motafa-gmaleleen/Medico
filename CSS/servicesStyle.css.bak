/* ----------------------- First Div ----------------------- */
#first {background-image:linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5)),url("../Images/bodymassca.png");height: 565px;width: 100%px;text-transform: uppercase;}
#first p {color: white;font-size: 70px;padding-left: 690px;padding-top: 110px;font-weight: 600}
#first span {color: white;font-size: 30px;padding-left: 1120px;font-weight: 600}

/* ----------------------- Second Div ----------------------- */

#second p{font-size: 50px;color: #25688e;text-align: center;margin: 50px ;text-transform: capitalize;}
#spans{font-size: 60px;color: #25688e;text-align: center;margin: 50px ;text-transform: capitalize; padding-top:10px;}
#second img { padding:0px 0px 10px 0px;display: inline-block;float:left;transition: .2s;}
#second img:hover{transform: scale(1.1);}

#second #discr { display: inline-block;background-color: #B6B1B6;margin:0px 0px 10px 0px;height: 300px;width: 969px}
#second #discr span {font-size: 28px;margin:0px;display: block; color: #2874af}
#second #discr p { margin: 10px 10px 0px 30px;float: left;font-size: 18px;line-height: 40px;display: inline-block;color: #333d4c}
#palign {text-align:left;}
#second #discr p a {text-decoration: none; color: #2874af;font-size: 22px}


/* ----------------------- third Div ----------------------- */


.rows {
display: flex;
background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));background-color:#25688e;    margin-top: -28px;
}

/* Create three equal columns that sits next to each other */
.column {
flex: 33.33%;
padding-left: 80px;
padding-right:80px;
margin-bottom:60px;
margin-top:20px;

}

#third { background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));background-color:#25688e;margin-top: 60px; position: relative;}

#third img { padding-left: 127px;padding-top: 50px; }
.column img:hover { transform: scale(1.1);}
#third p {color: white;font-size: 80px;text-align: center;padding-top:20px; padding-bottom:20px;}

#asty{display:inline-block;color: white;line-height: 25px;text-align:center;padding-top:15px;padding-bottom:15px;}
/* ----------------------- Fourth Div ----------------------- */
#pharmacy {background-color: #2874af;margin-top:60px;}
#pharmacy p{font-size: 50px;color: snow;text-align: center;text-transform: capitalize;padding: 50px 0px 0px 0px;font-weight: 600}

/* ----------------------- Calculate BIM ----------------------- */
#bim {background-image:linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5)),url("../Images/bodymasss.png");height: 100%;width: 100%;}
#bim h1{text-align: center;padding: 30px 0px 40px 0px;color: snow;font-size:50px;margin-top: 60px;}
#in {border: 1px solid #ccc;padding: 10px 30px 10px 30px;font-size: 18px;border-radius: 10px;margin-top:10px; }
#bim p {padding-bottom: 20px;padding:0px 20px 20px 0px; font-size: 20px;display: inline-block;font-size: 20px;color:whitesmoke}
#f{padding: 50px 0px 10px 0px}
#out {border: 1px solid #ccc;margin-left: 20px;padding: 5px;font-size: 18px;border-radius: 10px}
#out2 {border: 1px solid #ccc;padding: 5px;font-size: 18px;border-radius: 10px}
#butt {border: 1px solid #ccc;margin:20px 0px 30px 0px;background-color: #fff; color:#0c88cd;padding: 10px; font-size: 17px;border-radius: 15px }
#butt2  {border: 1px solid #ccc;margin:20px 0px 30px 0px;background-color: #fff; color:#0c88cd;padding: 10px 30px 10px 30px; font-size: 17px;border-radius: 15px }
.forms{display:block;text-align:center;}

/* ----------------------- Take Offers  -----------------------*/
#offer{text-align:center;}
#offer p {color: dimgray; font-size: 47px;font-weight: 600;margin-top:30px;margin-bottom:30px;}
#offer form input {border: 1px solid #ccc;width: 50%;height: 34px;background-color:white;font-size: 15px;border-radius: 5px;padding:6px 12px;float:center;}
#send {background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));
background-color: #25688e; 
color: white; 
font-size: 20px; 
width:200px;
height:50px;
border-radius:20px;
margin-bottom:80px;
border: 1px solid #ccc;
}



/* ----------------------- media tag -----------------------*/
