#first p{padding: 300px 0px 20px 0px;}
#first span{padding: 10px;font-size: 15px}
#second {width: 100%}
#second p {font-size: 50px}
#second img { width: 100% }
#second  #discr { width: 100%; }

/* ----------------------- third Div ----------------------- */
#third { width: 100% }
#third img { padding:0px 10% 0px 10%;width: 100%;}
#third #3links {display: none}  
#pharmacy {width:100%}

/* ----------------------- Calculate BIM ----------------------- */

#bim {width: 100%}
#bim h1{text-align: left;padding: 30px 0px 40px 50px;color: snow;font-size:40px}
#in { margin:20px 0px 10px 30%;padding: 10px;font-size: 18px;border-radius: 10px }
#bim p {padding-bottom: 20px;padding:0px 20px 20px 10%;display: inline-block;font-size: 18px;font-weight: 600;color:whitesmoke}
#out {margin-left: 20px;padding: 5px;font-size: 16px;border-radius: 10px}
#out2 {padding: 5px;font-size: 16px;border-radius: 10px}
#butt {margin:20px 0px 30px 35%;background-color: #fff; color:#0c88cd;padding: 10px; font-size: 17px;border-radius: 40px }
#butt2  {margin:20px 0px 30px 40%;background-color: #fff; color:#0c88cd;padding: 10px; font-size: 17px;border-radius: 40px }


/* ----------------------- Take Offers  -----------------------*/
#offer{text-align:center;}
#offer p { font-size: 50px;color:black;font-weight: 600;}
#offer form input {width: 300px; height: 30px;margin: 0px 20px 0px 0px;background-color:gainsboro;font-size: 20px;border-radius: 5px}
#send {     background-image: linear-gradient(rgba(0,0,0,0.5),rgba(0,0,0,0.5));
background-color: #74b2d4; color: white; font-size: 20px; width: 200px;height: 50px;border-radius: 40px;margin-bottom: 80px;margin-left: 50px}

#end {width: 100%;padding-left: 40% }


@media screen and (max-width:700px){
#first {width: 100%}
#first p{padding: 300px 0px 20px 30px;font-size: 30px}
#first span{padding-left: 40px;font-size: 15px}
#second {width: 100%}
