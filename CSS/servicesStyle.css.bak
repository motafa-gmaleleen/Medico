#pharmacy p {font-size: 30px}
#pharmacy img {padding: 10px 0px 20px 30px ;margin: 10px}
}
/* ----------------------- Take Offers  -----------------------*/


#last { width: 100%}
.links{width: 100%}
.links ul li{width: 100%;margin: 10px;}
.links ul li a { padding: 5px 10px; }
