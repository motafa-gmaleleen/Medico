<!-- ******************* First Div ******************* -->

<div id="first">
<p> Medico Registraion Form </p> 
<div id="box"> 
<form method="post"> 

<p id="bi">Personal Details</p> 
<label> UserName </label>
<input type="text" name="username" placeholder="Enter Your Username" size="10" maxlength="30" pattern="[A-Za-z]{3,30}" required> 
<label> Password</label> 
<input type="password" name="password" placeholder="Enter Your Password" size="30" maxlength="30" pattern=".{8,30}" required>
<label> Confirm Password</label>
<input type="password" name="repassword" placeholder="ReEnter Your Password" size="30" maxlength="30" pattern=".{8,30}" required> 
<label>BirthDay</label>
<input type="date" name="birthday" required> 
<label>Country</label>
<input type="text" name="country" placeholder="Enter Your Country Name" size="10" maxlength="30" pattern="[A-Za-z]{3,30}" required> 
<label>Phone Number</label>
<input type="tel" placeholder="Enter Your Phone Number" size="11" maxlength="30" pattern=".{11}" required> 
<p id="bi">Emergency Contact Details</p>
<label> Relative Name </label> 
<input type="text" name="username" placeholder="Enter Your Relative Name" size="10" maxlength="30" pattern="[A-Za-z]{3,30}" required> 
<label> Relationship </label> 
<input type="text" name="relation" placeholder="Relationship" size="10" maxlength="30" pattern="[A-Za-z]{3,30}" required>
<label>Emergency Phone Number</label>
<input type="tel" placeholder="Enter Emergency Phone Number" size="11" maxlength="30" pattern=".{11}" required>
<label>If Yes Write Shortly The Diseases Discribtion</label> 

<textarea cols="75" rows="8"></textarea> 
<button id="bu" type="submit" value="Submit" formaction="Login.html">Sign Up</button>	
</form>


